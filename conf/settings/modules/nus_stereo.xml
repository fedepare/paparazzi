<!DOCTYPE settings SYSTEM "../settings.dtd">

<settings>
  <dl_settings>
    <dl_settings NAME="NUS Stereo">
      <dl_setting var="nus_filter_factor" min="0" step="0.1" max="1" module="stereocam/stereocam" shortname="Filter Order"/>
  	  <dl_setting var="fit_thresh" min="0" step="1" max="255" module="stereocam/stereocam" shortname="FIT Thresh"/>
	    <dl_setting var="turn_cmd_max" min="0" step="5" max="100" module="stereocam/stereocam" shortname="MAX turn cmd" unit="%"/>
  	  <dl_setting var="climb_cmd_max" min="0" step="5" max="100" module="stereocam/stereocam" shortname="MAX climb cmd" unit="%"/>
	  <dl_setting var="redroplet_wait" min="0" step="0.5" max="10" module="stereocam/stereocam" shortname="redroplet time"/>
	  <dl_setting var="gate_count_thresh" min="0" step="1" max="50" module="stereocam/stereocam" shortname="gate count thresh"/>    
    </dl_settings>  
  </dl_settings>
</settings>
