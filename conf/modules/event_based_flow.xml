<!DOCTYPE module SYSTEM "module.dtd">

<module name="event_based_flow">
  <doc>
    <description>"Ventral flow and divergence estimation from event-based optic flow input"</description>
  </doc>
  <header>
    <file name="event_based_flow.h"/>
  </header>
  <init fun="event_based_flow_init()"/>
  <periodic fun="event_based_flow_periodic()" freq="100" start="event_based_flow_start()" stop="event_based_flow_stop()" autorun="FALSE"/>
  <makefile>
    <file name="event_based_flow.c"/>
  </makefile>
</module>

